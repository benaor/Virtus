{
  "expo": {
    "name": "Virtus",
    "slug": "virtus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "virtus",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.escarboucle.virtus",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.escarboucle.virtus",
      "adaptiveIcon": {
        "backgroundColor": "#FAF6EB",
        "foregroundImage": "./assets/images/android-icon-foreground.png"
      },
      "edgeToEdgeEnabled": true
    },
    "updates": {
      "url": "https://u.expo.dev/caadcb70-6094-4795-92ee-2f1aabd2b749"
    },
    "runtimeVersion": {
      "policy": "fingerprint"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./src/presentation/app"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FAF6EB"
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#8B6914"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "caadcb70-6094-4795-92ee-2f1aabd2b749"
      },
      "router": {
        "root": "./src/presentation/app"
      }
    },
    "owner": "benaor"
  }
}
